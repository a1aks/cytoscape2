    <AUTHORS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Matsumoto, T</AUTHORS>
    <AUTHORS rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eki, T</AUTHORS>
